{
  "name": "ventprom-packer",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tests/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "cd apps/api && npm run start:dev",
    "dev:web": "cd apps/web && npm run dev",
    "test": "echo 'Tests disabled for now'",
    "test:watch": "echo 'Tests disabled for now'",
    "e2e": "playwright test",
    "build": "npm run build:core && npm run build:parsers && npm run build:api && npm run build:web",
    "build:core": "cd packages/core && npm run build",
    "build:parsers": "cd packages/parsers && npm run build",
    "build:api": "cd apps/api && npm run build",
    "build:web": "cd apps/web && npm run build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "docker:up": "docker compose up",
    "docker:build": "docker compose build",
    "start:dev:api": "cd apps/api && npm run start:dev",
    "start:dev:web": "cd apps/web && npm run dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}